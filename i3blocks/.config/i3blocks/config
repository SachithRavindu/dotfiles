# Global properties
separator=true
separator_block_width=15


[spotify]
command=$HOME/.config/i3blocks/mediaplayer
interval=5

[cpu_mem]
command=~/.config/i3blocks/usage
interval=2

[time]
command=date '+%H:%M'
interval=60
#min_width=00:00 

[power]
command=~/.config/i3blocks/power
interval=once
min_width=power 

