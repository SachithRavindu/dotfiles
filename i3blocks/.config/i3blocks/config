# Global properties
separator=true
separator_block_width=15

[documentation]
full_text=Documentation
website=https://vivien.github.io/i3blocks
command=xdg-open "$website"
color=#f12711

[spotify]
command=$HOME/.config/i3blocks/mediaplayer
interval=2

[memory]
command=free -h | awk '/^Mem:/ {print "MEM "$3}'
interval=30

[time]
command=date '+%H:%M'
interval=60
min_width=00:00 



